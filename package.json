{
  "name": "fuga-svelte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:md": "markdownlint *.md",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,svelte,css,md,json}": [
      "prettier --write"
    ],
    "*.{js,svelte}": [
      "eslint --fix"
    ],
    "*.md": [
      "markdownlint --fix"
    ]
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.13.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "markdownlint-cli": "^0.46.0",
    "prettier": "^3.7.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.39.6",
    "svelte-eslint-parser": "^1.4.0",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "pitchy": "^4.1.0"
  }
}
